# Rujimi Configuration Example
# Copy this file to .env and update the values

# Basic Configuration
PASSWORD=123
WEB_PASSWORD=123
GEMINI_API_KEYS=your_api_key_1,your_api_key_2,your_api_key_3

# Server Configuration
PORT=7860

# Streaming Configuration
FAKE_STREAMING=true
FAKE_STREAMING_INTERVAL=1.0

# Concurrency Configuration
CONCURRENT_REQUESTS=1
INCREASE_CONCURRENT_ON_FAILURE=0
MAX_CONCURRENT_REQUESTS=3

# Cache Configuration
CACHE_EXPIRY_TIME=21600  # 6 hours in seconds
MAX_CACHE_ENTRIES=500
CALCULATE_CACHE_ENTRIES=6
PRECISE_CACHE=false

# Vertex AI Configuration
ENABLE_VERTEX=false
GOOGLE_CREDENTIALS_JSON=""
ENABLE_VERTEX_EXPRESS=false
VERTEX_EXPRESS_API_KEY=""

# Search Configuration
SEARCH_MODE=false
SEARCH_PROMPT="（使用搜索工具联网搜索，需要在content中结合搜索内容）"

# Security Configuration
RANDOM_STRING=true
RANDOM_STRING_LENGTH=5
MAX_EMPTY_RESPONSES=5
SHOW_API_ERROR_MESSAGE=true

# Rate Limiting Configuration
MAX_RETRY_NUM=15
MAX_REQUESTS_PER_MINUTE=30
MAX_REQUESTS_PER_DAY_PER_IP=600
API_KEY_DAILY_LIMIT=100

# Model Filtering Configuration
BLOCKED_MODELS=""
WHITELIST_MODELS=""
WHITELIST_USER_AGENT=""

# Other Configuration
PUBLIC_MODE=false
DASHBOARD_URL=""
ALLOWED_ORIGINS=""

# Storage Configuration
ENABLE_STORAGE=true
STORAGE_DIR=./rujimi_data

# Development Configuration
RUST_LOG=rujimi=info,tower_http=info